          FOR ALL ENTRIES IN lt_ekpo "i_mvke_sales
          WHERE kappl = 'V'
            AND kschl = 'XP00'
            AND vkorg = lt_ekpo-werks  "Assumption: Sales org which will be same as Plant WERKS
            AND matnr = lt_ekpo-matnr
            AND kfrst = ''
    AND datbi GE sy-datum.


