      SORT i_a905 BY matnr.

      SELECT knumh kbetr konwa
       INTO TABLE i_sales_4_a905
       FROM konp
       FOR ALL ENTRIES IN i_a905
       WHERE knumh = i_a905-knumh AND
      kschl = 'XP00'.
    ENDIF.
